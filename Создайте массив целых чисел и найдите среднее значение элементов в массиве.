
public class Main {
    public static void main(String[] args) {
        int[] numbers = {36, 29, 10, 27, 25};
        int sum = 0;
        double average;
        for (int number : numbers) {
            sum += number;
        }
        average = (double) sum / numbers.length;
        System.out.println("Среднее значение элементов в массиве: " + average);
    }

}
